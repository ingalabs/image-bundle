{
    "name": "ingalabs/image-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for image serving",
    "keywords": ["symfony-bundle", "bundle", "image"],
    "homepage": "https://github.com/ingalabs/image-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "IngaLabs",
            "email": "hello@ingalabs.hu",
            "homepage": "https://ingalabs.hu"
        },
        {
            "name": "Antal Áron",
            "email": "antalaron@antalaron.hu",
            "homepage": "https://antalaron.hu"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-gd": "*",
        "ext-exif": "*",
        "symfony/symfony": "4.4.*|5.0.*",
        "intervention/image": "^2.3",
        "twig/twig": "^2.0",
        "sybio/gif-frame-extractor": "^1.0",
        "sybio/gif-creator": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "matthiasnoback/symfony-dependency-injection-test": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "IngaLabs\\Bundle\\ImageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IngaLabs\\Bundle\\ImageBundle\\Tests\\": "tests/Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
